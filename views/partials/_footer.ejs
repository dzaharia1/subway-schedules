    </body>
    <script>
        <% if (signInfo.signId) { %>
            let signId = "<%= signInfo.signId %>";
        <% } %>
        async function APIRequest(verb, route) {
            let url = window.location.href;
            if (url.includes('https')) {
                url = `${window.location.href.substr(0, 8)}${url.split('/', 3)[2]}/`;
            } else {
                url = `${window.location.href.substr(0, 7)}${url.split('/', 3)[2]}/`;
            }
            url += `${route}`;
            console.log(url);
            let response = await fetch(url, { method: verb });
            let data = await response.json();
            return data;
        }
    </script>
